{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": false,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "baseUrl": "src",
        "paths": {
            "components/*": [
                "components/*"
            ],
            "pages/*": [
                "pages/*"
            ],
            "hooks/*": [
                "hooks/*"
            ],
            "contexts/*": [
                "contexts/*"
            ],
            "layout/*": [
                "layout/*"
            ],
            "models/*": [
                "models/*"
            ],
            "services/*": [
                "services/*"
            ],
            "utils/*": [
                "utils/*"
            ],
            "providers/*": [
                "providers/*"
            ],
            "reducers/*": [
                "reducers/*"
            ],
            "routes/*": [
                "routes/*"
            ],
        }
    },
    "include": [
        "./src"
    ]
}